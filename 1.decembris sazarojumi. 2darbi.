<!DOCTYPE HTML>

<html>

<head></head>
    
    <body>
  <script>
        
        var nauda = 10000;
      var precu_skaits = 10;
        var cena = 50;
         
      if (precu_skaits > 0){
          
          alert("Prece ir noliktava");
          
          
        
       if (cena < nauda){
          
          alert("Prece nopirkta");
          
          }  
      }
        
        </script>  
    
    
    
    
    </body>



</html>


